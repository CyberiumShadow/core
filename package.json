    "@typescript-eslint/parser": "^2.19.0",
    "ava": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-klasa": "dirigeants/klasa-lint",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
